@model IEnumerable<GestionUsarios.Models.TblMaterium>

@{
    ViewData["Title"] = "Index";
    Layout="~/Views/Shared/Template/_Layoutprivate.cshtml";
}

<h2>Index</h2>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.MtrNombre)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.MtrModulo)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.MtrCurso)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.FkPgmIdProgramaNavigation)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.MtrNombre)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.MtrModulo)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.MtrCurso)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.FkPgmIdProgramaNavigation.PkPgmIdPrograma)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.PkMtrIdMateria">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.PkMtrIdMateria">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.PkMtrIdMateria">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
